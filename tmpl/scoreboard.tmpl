{{- template "header" . }}
<h3 class="page-header">{{ title .Title }}</h3>
{{- with allTeamScores }}
<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>#</th>
            <th>Team Name</th>
            <th>Points</th>
        </tr>
    </thead>
    <tbody>
    {{- range . }}
        <tr>
            <td>{{ .Teamnumber }}</td>
            <td>{{ .Teamname }}</td>
            <td>{{ .Points }}</td>
        </tr>
    {{- end }}
    </tbody>
</table>
{{- end }}
{{- template "footer" . }}
