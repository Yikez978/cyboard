{{- template "header" . }}
<h3 class="page-header">{{ title .Title }}</h3>
<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>#</th>
            <th>Team Name</th>
            <th>Points</th>
        </tr>
    </thead>
    <tbody id="result-list">
    {{- range allTeamScores }}
        <tr id="{{ .Teamname }}">
            <td class="teamnumber">{{ .Teamnumber }}</td>
            <td class="teamname">{{ .Teamname }}</td>
            <td class="points">{{ .Points }}</td>
        </tr>
    {{- end }}
    </tbody>
</table>
{{- template "footer" . }}
